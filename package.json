{
	"version": "0.1.0",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"name": "worktree-manager",
	"author": "alexweininger",
	"module": "dist/worktree-manager.esm.js",
	"size-limit": [
		{
			"path": "dist/worktree-manager.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/worktree-manager.esm.js",
			"limit": "10 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^5.0.3",
		"@types/jsonfile": "^6.0.1",
		"husky": "^7.0.1",
		"size-limit": "^5.0.3",
		"tsdx": "^0.14.1",
		"tslib": "^2.3.1",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"git-worktree": "^0.1.1",
		"jsonfile": "^6.1.0",
		"simple-git": "^2.44.0",
		"untildify": "^4.0.0"
	}
}
